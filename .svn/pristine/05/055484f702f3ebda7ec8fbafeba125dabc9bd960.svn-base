<!DOCTYPE html>
<html class="um landscape min-width-240px min-width-320px min-width-480px min-width-768px min-width-1024px">
<head>
<title>晒单分享</title>
<meta charset="utf-8">
<meta name="viewport" content=" initial-scale=1, user-scalable=no, minimum-scale=1.0, maximum-scale=1.0">
<link href="../css/common.css" type="text/css" rel="stylesheet" />
<link rel="stylesheet" href="../css/style.css">

<script type="text/javascript" src="../js/jquery-1.9.1.js"></script>
<script type="text/javascript" src="../js/DateFormater.js"></script>
<script type="text/javascript" src="../js/template.js"></script>
<script type="text/javascript" src="../js/base/Main.js"></script>
<script type="text/javascript" src="../js/base/DataCenter.js"></script>
<script type="text/javascript" src="../js/base/JsonEx.js"></script>
<script type="text/javascript" src="../js/base/MessageMachine.js"></script>

<script type="text/javascript" src="../js/dm/DM_CommodityShares.js"></script>
</head>
<body class="bg">
<script type="text/javascript">
    var commoId;

    $(window).load = load(); 

    // 初始化加载数据
    function load(){
      // 获取请求参数
      commoId = getQueryString("commoId");

      if(commoId == null)
      {
        $toast("参数丢失");
        return;
      }

      // 加载晒单记录
      var dm_Shares = new DM_CommodityShares();
      var url = dm_Shares.url;
      url += commoId + ".json";

      dm_Shares.setUrl(url);

      // 提取数据更新至shoplist
      DataCenter.getDataAnsyc("shares", dm_Shares);      
    }

    // 晒单详情
    function openShareInfo(shareId)
    {
      var url = "sdxq.html?shareId=" + shareId;
      openWindow(url);
    }
</script>    

  <div id="shares" class="main">
  </div>
  <script type="text/javascript" src="../frame/frame.js"></script>
</body>
</html>
