/*通用样式*/
@charset "utf-8";
/*样式重置*/



a {
    text-decoration: none;
    color: #a4a4a4;
}

.clear {
    clear: both;
}

.clr10 {
    clear: both;
    height: 0.1rem;
    overflow: hidden;
    border-top: 1px solid #999999;
    background: #ffffff;
}

body {
    overflow-x: hidden;
  /*x轴超出的时候隐藏*/
    -webkit-tap-highlight-color: rgba(0,0,0,0);
 /*点击元素的高亮颜色，一般用于ios 移动设备*/
    font-family: Optima, Verdana,'Microsoft Yahei', sans-serif;
    width: 100%;
    height: 100%;
    position: relative;
}

p {
    font-size: 0.14rem;
    line-height: 146%;
}

img {
    border: none;
    width: 100%;
    max-width: 100%;
}
li{list-style:none};
i {
    font-style: normal;
}
/*轮播图*/
.wrap{
    width: 100%;
    height:155px;
    background-color: #f2f2f2;
}

#guide{
    -webkit-box-sizing: border-box;
    width: 100%;
}
.swipe {
    overflow: hidden;
    visibility: hidden;
    position: relative;
}
.swipe-wrap {
    overflow: hidden;
    position: relative;
}
.swipe-wrap img{
    width: 100%;  /*让我们的图片宽度是100%*/
    height: auto;
}
.swipe-wrap > figure {
    float: left;
    width: 100%;
    position: relative;
}
#circle{
position: absolute;
padding:2px 0 2px 6px;
bottom:5px;
right:10px;
overflow: hidden;
text-align: center;
/*background-color: rgba(0,0,0,.5);*/
border-radius: 999px;
/*transform: translate3d(0,0,0);*/
 }
#circle li{width: 8px;
 background-color: #fff;
 float: left; 
 margin-right:6px;
 height: 8px;
 border-radius: 50%;}
#circle li.current{background-color:#ef4917;}

.main {
    width: 3.75rem;
    margin: 0 auto;
    position: relative;
}
.banner-bottom {
    width: 3.75rem;
    position: fixed;
    bottom: 0;
    background: #fff;
    border-top: 1px solid #ececec;
    padding: 0.1rem 0;
}
.banner-bottom2 {
    width: 3.75rem;
    position: fixed;
    bottom: 0;
    background: #fff;
    border-top: 1px solid #ececec;
}
.banner-bottom2 .btn1{
    padding: 0.1rem 0;
    text-align: center;
    color: #fff;
    background-color: #fccb76;
}
.banner-bottom2 .btn2{
    padding: 0.1rem 0;
    text-align: center;
    color: #fff;
    background-color: #ff5269;
}

.gn_div {
    width: 3.75rem;
    background: #ffffff;
}

.gn_div .gn_ul {
    width: 3.45rem;
    height: 0.8rem;
    float: left;
    padding: 0.15rem;
