/*图片上传进度条*/
.upload{
    position: fixed;
    z-index: 10000;
    width: 2.75rem;
    margin: 0 auto;
    left: 0.5rem;
    top: 2rem;
    box-shadow: 0 0 5px #FFFFFF;
    -moz-box-shadow: 0 0 5px #FFFFFF;
    -webkit-box-shadow: 0 0 5px #FFFFFF;
    -o-box-shadow: 0 0 5px #FFFFFF;        
}
.upload .progress{
    position: relative;
    height: 0.2rem;
    border-radius: 0.04rem;
    background: #f0f0f0;
}
.upload .completed{
    position: absolute;
    top: 0;
    left: 0;
    height: 0.2rem;
    background: -moz-linear-gradient(left, #ffe599, #f88a1f);/*火狐*/ 
    background: -webkit-gradient(linear, 0% 0%, 100% 0%,from(#ffe599), to(#f88a1f));  /*谷歌*/ 
    background: -webkit-gradient(linear, 0% 0%, 100% 0%, from(#ffe599), to(#f88a1f));      /* Safari 4-5, Chrome 1-9*/
    background: -webkit-linear-gradient(left, #ffe599, #f88a1f);   /*Safari5.1 Chrome 10+*/
    background: -o-linear-gradient(left, #ffe599, #f88a1f);  /*Opera 11.10+*/
}
.upload .info{
    position: absolute;
    z-index: 10001;
    top: 0;
    left: 0;
    width: 2.75rem;
    text-align: center;
    font-size: 0.14rem;

}

