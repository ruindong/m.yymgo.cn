}

/************物品列表css**************/
.bwlb_div {
    width: 3.75rem;
    background: #ffffff;
}

.xz_ul {
    width: 3.55rem;
    padding: 0 0.1rem;
    font-size: 0;
    border-bottom: 1px solid #f1f1f1;
}

.xz_ul li {
    display: inline-block;
    width: 0.88rem;
