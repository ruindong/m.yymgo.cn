}

.zj_info {
    width: 2.9rem;
    padding: 0 0.15rem 0 0.7rem;
    height: 0.5rem;
    background: url(../images/lb-icon.png) 0.32rem center no-repeat;
    background-size: 0.3rem;
}

.box2 {
    width: 2.9rem;
    height: 0.5rem;
    overflow: hidden;
    position: relative;
/* padding-top:0.1rem;*/
}

.box2 .jdt a {
