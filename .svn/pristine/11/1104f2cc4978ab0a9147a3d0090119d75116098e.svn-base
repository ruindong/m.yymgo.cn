}

.cp_ul li:nth-child(2n) {
    border-right: none;
}

.cp_ul li .cp_pic {
    width: 1.2rem;
    display: block;
    margin: 0.1rem auto;
}

.cp_ul li .cp_name {
    width: 1.7rem;
    margin: 0.1rem auto;
    font-size: 0.14rem;
    line-height: 0.22rem;
    color: #333333;
    height: 0.44rem;
    overflow: hidden;
}

.cp_ul li .cp_cz {
    width: 1.85rem;
    height: 0.4rem;
}

.cp_ul li .cp_cz .jd_div {
    width: 1rem;
    height: 0.4rem;
    float: left;
    font-size: 0.12rem;
    margin-left: 0.08rem;
}

.cp_ul li .cp_cz .jd_div p {
    width: 1rem;
    height: 0.2rem;
    line-height: 0.2rem;
    font-size: 0.12rem;
    color: #999999;
}

.cp_ul li .cp_cz .jd_div p span {
    color: #96c9f6;
    padding-left: 0.02rem;
}

.cp_ul li .cp_cz .jd_div .jd_t {
    width: 0.9rem;
    height: 0.08rem;
    margin-top: 0.07rem;
    background: #ececec;
    border-radius: 0.04rem;
}

.cp_ul li .cp_cz .jd_div .jd_t span {
    display: block;
    width: 0;
    height: 0.08rem;
    background: #fdad16;
    border-radius: 0.04rem;
}

.cp_ul li .jrqd {
    font-size: 0.14rem;
    color: #e83754;
    display: block;
    width: 0.65rem;
    height: 0.3rem;
    line-height: 0.3rem;
    text-align: center;
    border: 1px solid #e83754;
    border-radius: 0.05rem;
    float: right;
    margin-right: 0.08rem;
    margin-top: 0.05rem;
}


