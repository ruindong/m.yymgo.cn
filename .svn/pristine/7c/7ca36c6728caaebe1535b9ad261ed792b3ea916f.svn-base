/*收缩菜单*/
.v {
	width: .14rem;
	height: .15rem;
	overflow: hidden;
	background: url(../image/vicon.png) no-repeat;
	display: inline-block;
	background-size: .15rem;
}

.v01 {
	background-position: 0 0;
}

.v02 {
	background-position: 0 -0.15rem;
}


.bzxq {
	width: 3.75rem;
	margin: 0 auto;
	padding: 0 .1rem;
	box-sizing: border-box;
}

.bzxq h3 {
	font-size: .16rem;
	text-align: center;
	line-height: .5rem;
	border-bottom: 1px solid #ededed;
}

.bzxq p {
	font-size: .14rem;
	line-height: .24rem;
}

.vcontent{
	margin-bottom: 0.1rem;
}
.vtitle{
	border-bottom: 1px solid #d0d0d0;
	padding: 0.05rem 0;
}
.vtitle .vtext{
	margin-right: 0.1rem;
	font-size: 0.14rem;
	color: #333;	
}
.vcon{
	font-size: 0.12rem;
	color: #666;
