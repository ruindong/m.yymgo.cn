/*通用样式*/
@charset "utf-8";

/*样式重置*/
* {
	padding: 0;
	margin: 0;
}

html {
	height: 100%;
}

body{
	font-size: 0.14rem;
    color: #333;
    line-height: 1.46;
    background-color: #ddd;
}

body,div,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,form, 
fieldset,input,textarea,p,blockquote,th,td {
	margin: 0;
	padding: 0;
}

/*安卓部分机型闪屏的问题*/
.backface{
    -webkit-backface-visibility:hidden; /* Chrome 和 Safari */   
}

/*淡入淡出*/
@-webkit-keyframes fadeIn {
	0% {
		opacity: 0.5; /*初始状态 透明度为0*/
	}
	30% {
		opacity: 1; /*中间状态 透明度为0*/
	}
	80% {
		opacity: 0.8; /*中间状态 透明度为0*/
	}	
	100% {
		opacity: 0.5; /*结尾状态 透明度为1*/
	}
}
.needFadeIn
{
	-webkit-animation-name: fadeIn; /*动画名称*/
	-webkit-animation-duration: 2.5s; /*动画持续时间*/
	-webkit-animation-iteration-count: 1; /*动画次数*/
	-webkit-animation-delay: 0s; /*延迟时间*/
}

button{
	border: none;
}
button:disabled{
    background-color:#999;
    color:#eaeaea;
}	

fieldset,img {
	border: 0;
}

address,caption,cite,code,dfn,em,strong,th,var {
	font-style: normal;
	font-weight: normal;
}
a{
	text-decoration:none;
}
ol,ul {
	list-style: none;
}

caption,th {
	text-align: left;
}

h1,h2,h3,h4,h5,h6 {
	font-size: 100%;
	font-weight: normal;
}

abbr,acronym {
	border: 0;
}
*:focus { outline: none; }
fieldset,img { border:0; }  
caption,th { text-align:left; } 

/**css-box*/
.ub
{
	display: -webkit-box !important;
	display: -moz-box !important;
	display: box !important;
}
.ur
{
	display: -webkit-box !important;
	display: -moz-box !important;
	display: box !important;
    font-size: 0.14rem;
}
.uc
{
	display: -webkit-box !important;
	display: -moz-box !important;
	display: box !important;
	position:relative;
	height: 0.5rem;
    line-height: 0.5rem;
    border-bottom: 1px solid #f1f1f1;
    font-size: 0.18rem;
    color: #333333;
    padding-left: 0.1rem;
}
.ud{
	display: -webkit-box !important;
	display: -moz-box !important;
	display: box !important;
	position:relative;
	height: 0.5rem;
    line-height: 0.5rem;
    border-bottom: 1px solid #f1f1f1;
    color: #333333; 
}
.ub-fh
{
	width:100%;
}
.ub-fv
{
	height:100%;
}
.ub-ver
{
	-webkit-box-orient:vertical;
	-moz-box-orient:vertical;	
	box-orient:vertical;
}
.ub-f1
{
	position:relative;
	-webkit-box-flex: 1; 
	-moz-box-flex: 1;
	box-flex: 1;
}
.ub-f2
{
	position:relative;
	-webkit-box-flex: 2;
	-moz-box-flex: 2;	
	box-flex: 2; 
	width: 1rem;

}

.ub-f3
{
	position:relative;
	-webkit-box-flex: 3;
	-moz-box-flex: 3;	
	box-flex: 3;
}

.ub-f4
{
	position:relative;
	-webkit-box-flex: 4;
	-moz-box-flex: 4;	
	box-flex: 4;
}

.ub-as {
    -webkit-box-align: start;
    box-align: start;
    -moz-box-align: start;    
}
.ub-ac
{
	-webkit-box-align:center;
	box-align:center;
    -moz-box-align: center;    	
}
.ub-ae
{
	-webkit-box-align:end;
	box-align:end;
    -moz-box-align: end;   	
}

.ub-ps
{
	-webkit-box-pack:start;
	box-pack:start;
	-moz-box-pack:start;	
}
.ub-pc
{
	-webkit-box-pack:center;
	box-pack:center;
	-moz-box-pack:center;		
}
.ub-pe
{
	-webkit-box-pack:end;
	box-pack:end;
	-moz-box-pack:end;			
}
.ub-pj
{
	-webkit-box-pack:justify;
	box-pack:justify;
	-moz-box-pack:justify;	
}

/**图片*/
.ub-img
{
	-webkit-background-size:contain;
	background-size:contain;
	background-repeat:no-repeat;
	background-position:center;
}

.ub-img1
{	
	-webkit-background-size:cover;
	background-size:cover;
	background-repeat:no-repeat;
	background-position:center;
}

